    "floo": "export NODE_ENV=floopool && nodemon",
    "bitgrin": "export NODE_ENV=bitgrin && nodemon",
    "debug": "export NODE_ENV=floopool && nodemon",
    "serve": "forever -f ./forever/production.json"
